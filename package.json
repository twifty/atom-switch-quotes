{
  "name": "switch-quotes",
  "main": "./lib/main",
  "version": "0.1.0",
  "description": "Quickly toggles the quote characters",
  "keywords": [
    "quotes",
    "replace",
    "editor",
    "toggle"
  ],
  "activationCommands": {
    "atom-text-editor": [
      "switch-quotes:toggle",
      "switch-quotes:cycle",
      "switch-quotes:scopes",
      "switch-quotes:highlight"
    ]
  },
  "repository": "https://github.com/twifty/atom-switch-quotes.git",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "season": "^6.0.2"
  }
}
